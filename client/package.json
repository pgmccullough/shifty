{
  "name": "shifty-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy": "aws s3 rm s3://shuft.app/ --recursive && npm run build && aws s3 sync dist/ s3://shuft.app --acl public-read && aws cloudfront create-invalidation --distribution-id E2PLF5AD1AVKBU --paths '/*'",
    "preview": "vite preview",
    "test-ct": "playwright test -c playwright-ct.config.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.27.1",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
